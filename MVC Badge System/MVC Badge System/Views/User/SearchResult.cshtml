<!--Don't use the standard layout for the user list-->
@model MVC_Badge_System.Models.SearchViewModel
    @{ 
        Layout = null;
    }

<datalist id="user_list">
    @foreach (var item in Model.SearchResults)
    {
        <!--First Name Last Name (email@email.com) w/ a hidden attribute that is the user ID-->
        <option value="@item.FirstName @item.LastName (@item.Email)" name="@item.UserId"/>
    }
</datalist>

